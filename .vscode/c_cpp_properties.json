{
    "configurations": [
        {
            "name": "stm32f4_boot",
            "includePath": [
                "boot",
                "platform/cmsis/core",
                "platform/cmsis/device",
                "platform/driver/inc",
                "boot/app/inc",
                "boot/driver/led",
                "platform/cmsis/device",
                "boot/driver/DMA",
                "third_lib/ringbuffer"
            ],
            "defines": [
                "STM32F40_41xxx",
                "USE_STDPERIPH_DRIVER",
                "DEBUG"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "intelliSenseMode": "linux-gcc-arm",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
